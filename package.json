{
  "name": "five-second-guess",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "bundle": "react-native bundle --dev false --platform android --entry-file index.js --bundle-output ./android/app/src/main/assets/index.android.bundle --assets-dest ./android/app/src/main/res",
    "gradle-clean": "cd android && ./gradlew clean && cd ..",
    "gradle-build": "cd android && ./gradlew build && cd ..",
    "gradle-release": "cd android && ./gradlew assembleRelease && cd ..",
    "gradle-debug": "cd android && ./gradlew assembleDebug && cd ..",
    "release-apk": "npm run gradle-clean && npm run gradle-release",
    "debug-apk": "npm run bundle && npm run gradle-clean && npm run gradle-debug",
    "rm-useless": "rm -rf android/app/src/main/res/drawable-* && rm -rf android/app/src/main/res/raw && rm -rf android/app/src/main/assets/*"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/cli": "^4.13.1",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-community/netinfo": "^5.9.9",
    "@react-native-community/picker": "^1.8.0",
    "@react-navigation/native": "^5.7.0",
    "@react-navigation/stack": "^5.7.0",
    "d3-shape": "^1.3.7",
    "expo-linear-gradient": "^8.2.1",
    "firebase": "^7.13.1",
    "i18n-js": "^3.7.1",
    "lodash.memoize": "^4.1.2",
    "metro-react-native-babel-transformer": "^0.64.0",
    "react": "16.13.1",
    "react-native": "^0.63.0",
    "react-native-cli": "^2.0.1",
    "react-native-firebase": "^5.6.0",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-iap": "^4.5.2",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-localize": "^1.4.2",
    "react-native-reanimated": "^1.9.0",
    "react-native-safe-area-context": "^3.1.1",
    "react-native-screens": "^2.9.0",
    "react-native-simple-toast": "^1.1.2",
    "react-native-svg": "^12.1.0",
    "react-native-swipe-cards": "^0.1.1",
    "react-native-vector-icons": "^7.0.0",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "babel-jest": "^25.1.0",
    "eslint": "^6.5.1",
    "jest": "^25.1.0",
    "metro-react-native-babel-preset": "^0.59.0",
    "react-test-renderer": "16.13.1"
  },
  "jest": {
    "preset": "react-native"
  }
}
